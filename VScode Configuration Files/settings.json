{
    "latex-workshop.latex.autoBuild.run": "never",
    // 禁用自动检测主文件，直接编译当前文件
    // "latex-workshop.latex.autoBuild.run": "onFileChange",
    "latex-workshop.latex.rootFile.doNotPrompt": true,
    "latex-workshop.showContextMenu": true,
    "latex-workshop.intellisense.package.enabled": true,
    "latex-workshop.message.error.show": false,
    "latex-workshop.message.warning.show": false,
    "latex-workshop.latex.tools": [
        {
            "name": "xelatex",
            "command": "xelatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOCFILE%"
            ]
        },
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOCFILE%"
            ]
        },
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-xelatex",
                "-outdir=%OUTDIR%",
                "%DOCFILE%"
            ]
        },
        {
            "name": "bibtex",
            "command": "bibtex",
            "args": [
                "%DOCFILE%"
            ]
        },
        {
            "name": "biber",
            "command": "biber",
            "args": [
                "%DOCFILE%"
            ]
        }
    ],
    "latex-workshop.latex.recipes": [
        {
            "name": "LaTeXmk",
            "tools": [
                "latexmk"
            ]
        },
        {
            "name": "XeLaTeX -> Biber -> XeLaTeX*2",
            "tools": [
                "xelatex",
                "biber",
                "xelatex",
                "xelatex"
            ]
        },
        {
            "name": "PDFLaTeX -> Biber -> PDFLaTeX*2",
            "tools": [
                "pdflatex",
                "biber",
                "pdflatex",
                "pdflatex"
            ]
        },
        {
            "name": "XeLaTeX",
            "tools": [
                "xelatex"
            ]
        },
        {
            "name": "PDFLaTeX",
            "tools": [
                "pdflatex"
            ]
        },
        {
            "name": "BibTeX",
            "tools": [
                "bibtex"
            ]
        },
        {
            "name": "XeLaTeX -> Bibtex -> XeLaTeX*2",
            "tools": [
                "xelatex",
                "bibtex",
                "xelatex",
                "xelatex"
            ]
        },
        {
            "name": "PDFLaTeX -> Bibtex -> PDFLaTeX*2",
            "tools": [
                "pdflatex",
                "bibtex",
                "pdflatex",
                "pdflatex"
            ]
        },
    ],
    "latex-workshop.latex.clean.fileTypes": [
        "*.aux",
        "*.bbl",
        "*.blg",
        "*.idx",
        "*.ind",
        "*.lof",
        "*.lot",
        "*.out",
        "*.toc",
        "*.acn",
        "*.acr",
        "*.alg",
        "*.glg",
        "*.glo",
        "*.gls",
        "*.ist",
        "*.fls",
        "*.log",
        "*.fdb_latexmk"
    ],
    "latex-workshop.latex.autoClean.run": "never", //never:表示永远不自动清理上述冗杂文件；onFail:表示编译失败时自动清理上述冗杂文件；onBuilt:表示编译成功时自动清理上述冗杂文件
    "latex-workshop.latex.recipe.default": "lastUsed",// 设置默认配方（上一次使用的配方自动设置为默认配方）
    "latex-workshop.view.pdf.internal.synctex.keybinding": "double-click",
    "workbench.settings.applyToAllProfiles": [
        "editor.fontFamily"
    ],
    "editor.fontLigatures": false,
    "editor.fontFamily": "·Consolas, 'Courier New', monospace",
    "files.autoSave": "afterDelay",
    "editor.accessibilitySupport": "off",
    "[jsonc]": {
        "editor.quickSuggestions": {
            "strings": true
        },
        "editor.suggest.insertMode": "replace"
    },
    "editor.stickyScroll.enabled": false,
    "diffEditor.ignoreTrimWhitespace": true,
    "search.followSymlinks": false,
    "editor.wordWrap": "on",
    "workbench.secondarySideBar.showLabels": false,
    "workbench.experimental.share.enabled": true,
    "window.customTitleBarVisibility": "windowed",
    "github.copilot.nextEditSuggestions.enabled": false,
    "github.copilot.enable": {
        "*": false,
        "plaintext": false,
        "markdown": false,
        "scminput": false
    },
    "editor.formatOnSave": true,
    "editor.unicodeHighlight.allowedCharacters": {
        " ": true
    },
    "files.autoGuessEncoding": true,
    "workbench.secondarySideBar.defaultVisibility": "hidden"
}